#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/s/Cohort/G29-PARKit/node_modules/tree-kill/node_modules:/mnt/s/Cohort/G29-PARKit/node_modules:/mnt/s/Cohort/node_modules:/mnt/s/node_modules:/mnt/s/Cohort/G29-PARKit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/s/Cohort/G29-PARKit/node_modules/tree-kill/node_modules:/mnt/s/Cohort/G29-PARKit/node_modules:/mnt/s/Cohort/node_modules:/mnt/s/node_modules:/mnt/s/Cohort/G29-PARKit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tree-kill/cli.js" "$@"
else
  exec node  "$basedir/../tree-kill/cli.js" "$@"
fi
